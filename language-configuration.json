{
	"comments": {
		"lineComment": "\\#",
		"blockComment": [ "", "" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["[","]"],
		["(",")"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["'", "'"]
	],
	"folding": {
        "offSide": true,
        "markers": {
            "start": "^.*\\:",
            "end": "^.*END"
        }
	}
}