{
	"comments": {
		"lineComment": "#",
		"blockComment": [ "", "" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		{ "open": "[", "close": "]" },
		{ "open": "(", "close": ")" },
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["'", "'"]
	],
	"folding": {
        "offSide": true,
        "markers": {
            "start": ":",
            "end": "END"
        }
	}
}