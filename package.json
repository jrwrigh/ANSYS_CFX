{
    "name": "cfxlanguage",
    "displayName": "ANSYS CFX Language",
    "description": "Language Support for ANSYS CFX based scripts (CFX Command Language, CFX Expression Language, etc.). Applies for pre- and post-processing of CFX simulations.",
    "version": "0.1.0",
    "publisher": "tbd",
    "engines": {
        "vscode": "^0.19.0"
    },
    "categories": [
        "Languages"
    ],
    "author": {
        "name": "u2berggeist"
    },
    "contributes": {
        "languages": [{
            "id": "CCL",
            "aliases": ["CCL","CEL","CFX Command Language"],
            "extensions": [".ccl",".cst"],
            "configuration":"./language-configuration.json"
        }],
        "grammars": [{
            "language": "CCL",
            "scopeName": "source.ccl",
            "path": "./syntaxes/ccl.tmLanguage.json"
        }]
    }
}